<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="EMPLOYEE" schema="SYSTEM">
		<primary-key>
			<key-column name="EMP_ID" property="employeeId" />
		</primary-key>
		<column name="TEAM" property="team"></column>
		<column name="FIRST_NAME" property="firstName"></column>
		<column name="LAST_NAME" property="lastName"></column>
		<column name="NT_LOGIN" property="ntLogin"></column>
		<column name="IS_ACTIVE" property="isActive"></column>
		<column name="MOBILE" property="mobile"></column>
		<column name="EMAIL" property="email"></column>
		<column name="MANAGER" property="manager"></column>
		<column name="ISMANAGER" property="isManager"></column>
	</table>
	<table name="PROPERTIES_LOV" schema="SYSTEM">
		<primary-key>
			<key-column name="ID" property="id" />
		</primary-key>
		<column name="KEY" property="key"></column>
		<column name="TYPE" property="type"></column>
		<column name="VALUE" property="value"></column>
	</table>
</hibernate-reverse-engineering>