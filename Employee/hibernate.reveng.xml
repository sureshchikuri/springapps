<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="EMPLOYEE" schema="SYSTEM">
		<primary-key>
			<key-column name="EMPID" property="empId" />
		</primary-key>
		<column name="TEAM"></column>
		<column name="FIRSTNAME" property="firstName"></column>
		<column name="LASTNAME" property="lastName"></column>
		<column name="NTLOGIN" property="ntLogin"></column>
		<column name="IS_ACTIVE" property="isActive"></column>
	</table>
</hibernate-reverse-engineering>